# LLM Provider Configuration (anthropic, gemini, or custom)
LLM_PROVIDER=anthropic

# Anthropic API Key
ANTHROPIC_API_KEY=your-anthropic-api-key-here

# Gemini API Key
GEMINI_API_KEY=your-gemini-api-key-here

# Custom LLM Configuration (e.g., Open WebUI, LocalAI, vLLM, etc.)
# Set LLM_PROVIDER=custom to use your self-hosted LLM
CUSTOM_LLM_URL=http://your-llm-server:3000
CUSTOM_LLM_USERNAME=your-email@example.com
CUSTOM_LLM_PASSWORD=your-password

# SiteMinder Configuration
SITEMINDER_BASE_URL=https://casso.cx.anapartner.net
SITEMINDER_USER=siteminder
SITEMINDER_PASSWORD=anaPassword01

# Backend Server Port
PORT=3000
HOST=0.0.0.0
